LOAD DATA LOCAL INFILE 'courseDemo.csv' INTO TABLE courses FIELDS TERMINATED BY ',' ENCLOSED BY '\"' LINES TERMINATED BY '\n' (course_code,course_name);

LOAD DATA LOCAL INFILE 'course_subjects.csv' INTO TABLE course_subjects FIELDS TERMINATED BY ',' ENCLOSED BY '\"' LINES TERMINATED BY '\n' (course_id,subject_id);



retrieve single column: TableName.pluck(:columnName) : Person.pluck(:id)
retrieve single column unique value: TableName.uniq.pluck(:columnName) : Person.uniq.pluck(:id)
