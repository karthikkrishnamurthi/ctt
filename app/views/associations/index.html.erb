<p id="notice"><%= notice %></p>

<h1>Associations - <%= @associations.count %></h1>

<table>
  <thead>
    <tr>
    <th>ID</th>
      <th>Courses</th>
      <th>Subjects</th>
     <th>Semester</th>
      <th>Location</th>
      <th>Programme</th>
      <th>Subject type</th>
      <th>Subject category</th>
      <th>Course type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @associations.each do |association| %>
      <tr>
      <td><%= association.id %></td>
        <td><%= association.course.course_name %></td>
        <td><%= association.subject.id %></td>
        <td><%= association.semester.name %></td>
        <td><%= association.location.name %></td>
        <td><%= association.programme.name %></td>
        <td><%= association.subject_type.name %></td>
        <td><%= association.subject_category.name %></td>
        <td><%= association.course_type.name %></td>
        <td><%= link_to 'Edit', edit_association_path(association) %></td>
       <!--  <td><%#= link_to 'Show', association %></td>
        <td><%#= link_to 'Edit', edit_association_path(association) %></td>
        <td><%#= link_to 'Destroy', association, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Association', new_association_path %>
